<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pharmacyinterface">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pharmacylogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pharmacylogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.PharmacyInterface">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".SearchMed" />
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".Shop" />

        <activity android:name=".Register" />

        <activity android:name=".ShowProfile" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>